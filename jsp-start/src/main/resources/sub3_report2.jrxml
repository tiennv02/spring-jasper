	<queryString>
		<![CDATA[select a.*,
substring(a.TENCAN,7,9) STT,
CASE substring(a.TENCAN,7,9)
  WHEN '1' THEN 'THAN 1'
  WHEN '2' THEN 'THAN 2'
  WHEN '3' THEN 'Q.LẬT -100'
  WHEN '4' THEN 'THAN 4'
  WHEN '5' THEN 'THAN 5'
  WHEN '6' THEN 'THAN 6'
  WHEN '7' THEN 'THAN 7'
  WHEN '8' THEN 'THAN 8'
  WHEN '9' THEN 'THAN 9'
  WHEN '10' THEN 'THAN 10'
  WHEN '11' THEN 'CG+XLM'
  WHEN '12' THEN ''
  ELSE ''
END as THAN,
CASE substring(a.TENCAN,7,9)
  WHEN '1' THEN 'THAN 1'
  WHEN '2' THEN 'THAN 2'
  WHEN '3' THEN 'Q.LẬT -100'
  WHEN '4' THEN 'THAN 4'
  WHEN '5' THEN 'THAN 5'
  WHEN '6' THEN 'THAN 6'
  WHEN '7' THEN 'THAN 7'
  WHEN '8' THEN 'THAN 8'
  WHEN '9' THEN 'THAN 9'
  WHEN '10' THEN 'THAN 10'
  WHEN '11' THEN 'CG+XLM'
  WHEN '12' THEN ''
  ELSE ''
END as THAN
 from (
SELECT
      	  TENCAN,
	  MAX(GIATRI) MAX_GIATRI,
	  MIN(GIATRI) MIN_GIATRI,
	  (case when MAX(GIATRI) = 0 then 0 else MAX(GIATRI) - MIN(GIATRI) end) as LECH
  FROM
  DBO.DULIEU1 d
  WHERE thoigian BETWEEN convert(datetime, $P{p_Date},121 ) AND convert(datetime, $P{p_endDate},121)
	--and TENCAN != 'Can So 12'
  GROUP BY TENCAN
  ) a
ORDER BY CAST(substring(a.TENCAN,7,9) as int)]]>
	</queryString>
