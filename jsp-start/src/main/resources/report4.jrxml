	<queryString>
		<![CDATA[select a.*,
CASE a.TENCAN
  WHEN 'Can So 1' THEN 'THAN 1'
  WHEN 'Can So 2' THEN 'THAN 2'
  WHEN 'Can So 3' THEN 'THAN 3'
  WHEN 'Can So 4' THEN 'THAN 4'
  WHEN 'Can So 5' THEN 'THAN 5'
  WHEN 'Can So 6' THEN 'THAN 6'
  WHEN 'Can So 7' THEN 'THAN 7'
  WHEN 'Can So 8' THEN 'THAN 8'
  WHEN 'Can So 9' THEN 'THAN 9'
  WHEN 'Can So 10' THEN 'THAN 10'
  WHEN 'Can So 11' THEN N'ĐƠN VỊ ĐÀO LÒ'
  WHEN 'Can So 12' THEN 'KHO +38'
  ELSE ''
END as THAN
 from (
SELECT
      TENCAN,
	  MAX(GIATRI) MAX_GIATRI,
	  MIN(GIATRI) MIN_GIATRI,
	  MAX(GIATRI) - MIN(GIATRI) LECH
  FROM    DBO.DULIEU1 d
  WHERE thoigian  BETWEEN convert(datetime, $P{p_startDate},121) AND convert(datetime, $P{p_endDate},121)
  GROUP BY TENCAN
  ) a
ORDER BY CAST(substring(a.TENCAN,7,9) as int)]]>
	</queryString>
