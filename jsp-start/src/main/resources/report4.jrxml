	<queryString>
		<![CDATA[select a.*,
CASE STT
  WHEN '1' THEN 'THAN 1'
  WHEN '2' THEN 'THAN 2'
  WHEN '3' THEN 'THAN 3'
  WHEN '4' THEN 'THAN 4'
  WHEN '5' THEN 'THAN 5'
  WHEN '6' THEN 'THAN 6'
  WHEN '7' THEN 'THAN 7'
  WHEN '8' THEN 'THAN 8'
  WHEN '9' THEN 'THAN 9'
  WHEN '10' THEN 'THAN 10'
  WHEN '11' THEN N'ĐƠN VỊ ĐÀO LÒ'
  WHEN '12' THEN 'KHO +38'
  ELSE ''
END as THAN
 from (
	select a.STT, b.TENCAN, b.MAX_GIATRI, b.MIN_GIATRI, (MAX_GIATRI - MIN_GIATRI) as LECH
	from
		(select top 12 row_number() over (order by ID) as STT from DBO.DULIEU1) a
		left join
		(
			SELECT
			  TENCAN,
			  substring(TENCAN,7,9) as STT,
			  MAX(GIATRI) MAX_GIATRI,
			  MIN(GIATRI) MIN_GIATRI

			FROM
			  DBO.DULIEU1 d
			WHERE thoigian BETWEEN convert(datetime, $P{p_startDate},121 ) AND convert(datetime, $P{p_endDate},121)
			GROUP BY TENCAN,  substring(TENCAN,7,9)
		)b on a.STT = b.STT
  ) a
--where a.STT != '12'
ORDER BY CAST(STT as int)]]>
	</queryString>
