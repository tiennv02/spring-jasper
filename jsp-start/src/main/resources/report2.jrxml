	<parameter name="p_tenCanTV" class="java.lang.String"/>
	<parameter name="p_title" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
TENCAN,
CA,
GIATRI_BD,
GIATRI_CUOI,
THOIGIAN_DAU,
THOIGIAN_KT,
GIATRI_CUOI - GIATRI_BD as LECH FROM (
SELECT
      TENCAN,
	 $P{p_ca} CA,
	  GIATRI GIATRI_BD,
	  (SELECT MIN(GIATRI) FROM DBO.DULIEU1 A WHERE A.GIATRI > DL.GIATRI and  upper(DL.TENCAN) =upper(A.TENCAN)) GIATRI_CUOI,
	  THOIGIAN THOIGIAN_DAU,
	  (SELECT MIN(THOIGIAN) FROM DBO.DULIEU1 A WHERE A.THOIGIAN > DL.THOIGIAN  and  upper(DL.TENCAN) =upper(A.TENCAN)) THOIGIAN_KT
  FROM DBO.DULIEU1 DL
 WHERE
 upper(trim(DL.TENCAN)) =upper(trim($P{p_can}))
and dl.thoigian BETWEEN CONVERT(datetime, $P{p_startDate}, 121) AND CONVERT(datetime,$P{p_endDate}, 121)
) DL

 ORDER BY GIATRI_BD asc;



]]>
	</queryString>
